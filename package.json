    "@tinymce/eslint-plugin": "^1.9.1",
    "@tinymce/miniature": "^3.2.0",
    "@types/node": "^16.7.1",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.2.29",
    "babel-loader": "^8.2.3",
    "babel-preset-vue": "^2.0.2",
    "babel-register": "^6.26.0",
    "css-loader": "^6.2.0",
    "file-loader": "^6.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
