{
  "name": "@tinymce/tinymce-vue",
  "version": "1.0.9",
  "description": "Official TinyMCE Vue Component",
  "private": false,
  "repository": {
    "url": "https://github.com/tinymce/tinymce-vue"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/es2015/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf lib",
    "lint": "tslint src/**/*.ts",
    "build": "npm run clean && npm run lint && tsc -p ./tsconfig.es2015.json && tsc -p ./tsconfig.cjs.json && node rollup.build.js",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "keywords": [
    "tinymce",
    "vue",
    "component"
  ],
  "author": "Ephox Inc",
  "license": "Apache-2.0",
  "files": [
    "lib/*/**",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.txt"
  ],
  "dependencies": {
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@storybook/vue": "^3.3.11",
    "@types/node": "^9.4.0",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.9",
    "file-loader": "^1.1.6",
    "rimraf": "^2.6.2",
    "rollup": "^0.55.3",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-typescript2": "^0.10.0",
    "rollup-plugin-uglify": "^3.0.0",
    "ts-loader": "^3.4.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.8.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.7.1",
    "vue-loader": "^14.0.3",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0"
  }
}
