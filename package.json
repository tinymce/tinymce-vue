{
  "name": "@tinymce/tinymce-vue",
  "version": "1.1.0",
  "description": "Official TinyMCE Vue Component",
  "private": false,
  "repository": {
    "url": "https://github.com/tinymce/tinymce-vue"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/es2015/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf lib",
    "lint": "tslint src/**/*.ts",
    "build": "npm run clean && npm run lint && tsc -p ./tsconfig.es2015.json && tsc -p ./tsconfig.cjs.json && node rollup.build.js",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "keywords": [
    "tinymce",
    "vue",
    "component"
  ],
  "author": "Ephox Inc",
  "license": "Apache-2.0",
  "files": [
    "lib/*/**",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.txt"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@storybook/storybook-deployer": "^2.3.0",
    "@storybook/vue": "^4.0.4",
    "@types/node": "^10.12.6",
    "babel-loader": "^8.0.4",
    "babel-preset-vue": "^2.0.2",
    "babel-register": "^6.26.0",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript2": "^0.18.0",
    "rollup-plugin-uglify": "^6.0.0",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.1.6",
    "vue": "^2.5.21",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.25.1"
  }
}
