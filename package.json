{
  "name": "@tinymce/tinymce-vue",
  "version": "2.1.0",
  "description": "Official TinyMCE Vue Component",
  "private": false,
  "repository": {
    "url": "https://github.com/tinymce/tinymce-vue"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/es2015/index.js",
  "scripts": {
    "test": "bedrock-auto -b chrome-headless -f src/test/ts/**/*Test.ts",
    "test-manual": "bedrock -f src/test/ts/**/*Test.ts",
    "clean": "rimraf lib",
    "lint": "tslint src/**/*.ts",
    "build": "yarn run clean && yarn run lint && tsc -p ./tsconfig.es2015.json && tsc -p ./tsconfig.cjs.json && node rollup.build.js",
    "prepare": "yarn run build",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "keywords": [
    "tinymce",
    "vue",
    "component"
  ],
  "author": "Ephox Inc",
  "license": "Apache-2.0",
  "files": [
    "lib/*/**",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.txt"
  ],
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@ephox/agar": "^4.13.10",
    "@ephox/bedrock": "^4.3.0",
    "@ephox/sugar": "^4.6.21",
    "@ephox/tslint-rules": "^1.0.11",
    "@storybook/addon-notes": "^4.1.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/vue": "^4.1.6",
    "@types/node": "^10.12.18",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "babel-register": "^6.26.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "rollup-plugin-uglify": "^6.0.0",
    "tinymce": "^5.0.7",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "vue-loader": "^15.5.1",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.1"
  }
}
